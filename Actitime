	System.setProperty("webdriver.chrome.driver", "./Drivers/chromedriver.exe");
		WebDriver driver=new ChromeDriver();
		driver.manage().window().maximize();
		driver.get("https://demo.actitime.com/login.do");
		WebElement ele=driver.findElement(By.name("username"));
		ele.sendKeys("admin");
	    WebElement pass = driver.findElement(By.name("pwd"));
	    pass.sendKeys("manager");
	    driver.findElement(By.id("keepLoggedInLabel")).click();
	    driver.findElement(By.xpath("//div[text()='Login ']")).click();
	    driver.findElement(By.className("content tasks")).click();
	    driver.findElement(By.className("content reports")).click();
	    driver.findElement(By.className("content users")).click();
          driver.findElement(By.className("content users")).click();